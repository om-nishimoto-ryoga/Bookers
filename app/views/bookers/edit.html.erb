<h1>Editing Book</h1>
<%= form_for(@bookers) do |f| %>
    <h4>Title</h4>
    <div><%= f.text_field :title, class:"book_title" %></div>
    <h4>Body</h4>
    <div><%= f.text_area :body, class:"book_body" %></div>
    <div><%= f.submit 'Update Book' %> </div>
<% end %>
<%= link_to "Show", booker_path(@bookers), class:"show_#{@bookers.id}" %>
 | 
<%= link_to "Back", bookers_path, class:"back" %>